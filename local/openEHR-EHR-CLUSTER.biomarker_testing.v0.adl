definition
	CLUSTER[at0000] matches {    -- Biomarker testing for breast carnicoma
		items cardinality matches {1..*; unordered} matches {
			CLUSTER[at0001] occurrences matches {0..1} matches {    -- Oestrogen Receptor assay (ER)
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- ER result
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0014,    -- Postitive
									at0015,    -- Negative
									at0016]    -- Equivocal
								}
							}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Proportion of nuclei stained
						value matches {
							DV_PROPORTION matches {
								type matches {2}
							}
						}
					}
					ELEMENT[at0032] occurrences matches {0..1} matches {    -- Predominant staining intensity
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0034,    -- Low
									at0035,    -- Intermediate
									at0036]    -- High
								}
							}
						}
					}
				}
			}
			CLUSTER[at0002] occurrences matches {0..1} matches {    -- Progesterone Receptor assay (PgR)
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- PgR result
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0017,    -- Positive
									at0018,    -- Negative
									at0019]    -- Equivocal
								}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- Proportion of nuclei stained
						value matches {
							DV_PROPORTION matches {
								type matches {2}
							}
						}
					}
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Predominant staining intensity
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0037,    -- Low
									at0038,    -- Intermediate
									at0039]    -- High
								}
							}
						}
					}
				}
			}
			CLUSTER[at0003] occurrences matches {0..1} matches {    -- Human Oestrogen Receptor 2 assay by immunohistochemistry (HER2 IH)
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0010] occurrences matches {0..1} matches {    -- HER2 IH score
						value matches {
							1|[local::at0026],
							2|[local::at0027],
							3|[local::at0028],
							4|[local::at0029],
							5|[local::at0030]
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {    -- HER2 IH result
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0020,    -- Positive
									at0021,    -- Negative
									at0022]    -- Equivocal
								}
							}
						}
					}
				}
			}
			CLUSTER[at0004] occurrences matches {0..1} matches {    -- HER2 in situ hybridisation
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0012] occurrences matches {0..1} matches {    -- ISH HER2 result
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0023,    -- Postive
									at0024,    -- Negative
									at0025,    -- Not performed
									at0031]    -- Can not be determined
								}
							}
							DV_TEXT matches {*}
						}
					}
				}
			}
			CLUSTER[at0005] occurrences matches {0..1} matches {    -- Ki67
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Percentage of nuclei stained (in hotspot)
						value matches {
							DV_PROPORTION matches {
								type matches {2}
							}
						}
					}
				}
			}
		}
	}

