definition
	CLUSTER[at0000] matches {    -- total calories activity_period
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0026] occurrences matches {0..1} matches {    -- Year
				value matches {
					DV_DATE matches {
						value matches {yyyy-??-XX}
					}
				}
			}
			CLUSTER[at0028] occurrences matches {0..1} matches {    -- Per period
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0030] occurrences matches {0..1} matches {    -- Calender period
						value matches {
							DV_TEXT matches {*}
							DV_CODED_TEXT matches {*}
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {    -- Defined date for the period
						value matches {
							DV_DATE matches {
								value matches {yyyy-mm-??}
							}
						}
					}
					ELEMENT[at0001] occurrences matches {0..1} matches {    -- calendar period
						value matches {
							1|[local::at0011],
							2|[local::at0012],
							3|[local::at0013],
							4|[local::at0014]
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Time interval during a specific period
						value matches {
							DV_DURATION matches {
								value matches {|P1D..P1Y|}
							}
						}
					}
					CLUSTER[at0003] occurrences matches {0..*} matches {    -- Per activity
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0015] occurrences matches {0..1} matches {    -- Type of activity
								value matches {
									5|[local::at0016],
									6|[local::at0017],
									7|[local::at0018],
									8|[local::at0019],
									9|[local::at0020],
									10|[local::at0021],
									11|[local::at0022],
									12|[local::at0023],
									13|[local::at0024]
								}
							}
							ELEMENT[at0005] occurrences matches {0..1} matches {    -- Frequency
								value matches {
									C_DV_QUANTITY <
										
										list = <
											["1"] = <
												units = <"min/d">
											>
											["2"] = <
												units = <"min/wk">
											>
											["3"] = <
												units = <"min/mo">
											>
										>
									>
								}
							}
							ELEMENT[at0043] occurrences matches {0..1} matches {    -- Exertion factor
								value matches {
									C_DV_QUANTITY <
										
										list = <
											["1"] = <
												units = <"kcal/min">
												magnitude = <|>2.0..15.0|>
												precision = <|0|>
											>
										>

										assumed_value = <
											units = <"kcal/min">
											magnitude = <3.0>
											precision = <0>
										>
									>
								}
							}
							allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {    -- CLUSTER_SLOT
								include
									archetype_id/value matches {/.*/}
							}
						}
					}
					ELEMENT[at0029] occurrences matches {0..1} matches {    -- Sum activity calories per per period
						value matches {
							DV_COUNT matches {*}
						}
					}
				}
			}
			ELEMENT[at0006] occurrences matches {0..1} matches {    -- Total activity calories
				value matches {
					DV_COUNT matches {*}
				}
			}
		}
	}

