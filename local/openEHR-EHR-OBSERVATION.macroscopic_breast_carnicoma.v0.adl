												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0073] occurrences matches {0..1} matches {    -- Tumour properties
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0096] occurrences matches {0..1} matches {    -- Number of tumours
												value matches {
													DV_COUNT matches {*}
												}
											}
											ELEMENT[at0079] occurrences matches {0..1} matches {    -- Appearance
												value matches {
													DV_TEXT matches {*}
												}
											}
											allow_archetype CLUSTER[at0110] occurrences matches {0..*} matches {    -- Tumour size
												include
													archetype_id/value matches {/.*|openEHR-EHR-CLUSTER\.physical_properties\.v0/}
											}
											ELEMENT[at0080] occurrences matches {0..1} matches {    -- Distance from tumour centre to mamill
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::122]>
														list = <
															["1"] = <
																units = <"mm">
																magnitude = <|>=0.0|>
															>
														>
													>
												}
											}
											allow_archetype CLUSTER[at0111] occurrences matches {0..*} matches {    -- Distance from tumour periphery to resection surface
												include
													archetype_id/value matches {/.*|openEHR-EHR-CLUSTER\.tumour_resection_margins-Test2018\.v0/}
											}
											ELEMENT[at0112] occurrences matches {0..1} matches {    -- Additional information
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
									ELEMENT[at0105] occurrences matches {0..1} matches {    -- Övrigt
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
