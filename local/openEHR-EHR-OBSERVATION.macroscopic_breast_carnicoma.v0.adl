												include
													archetype_id/value matches {/.*|openEHR-EHR-CLUSTER\.specimen\.v0/}
											}
											allow_archetype CLUSTER[at0106] occurrences matches {0..*} matches {    -- Specimen laterality
												include
													archetype_id/value matches {/.*|openEHR-EHR-CLUSTER\.anatomical_location\.v1/}
											}
											allow_archetype CLUSTER[at0109] occurrences matches {0..*} matches {    -- Specimen dimensions
												include
													archetype_id/value matches {/.*|openEHR-EHR-CLUSTER\.physical_properties\.v0/}
											}
											ELEMENT[at0070] occurrences matches {0..1} matches {    -- Mamill
