definition
	CLUSTER[at0000] matches {    -- Distance activity details
		items cardinality matches {1..*; unordered} matches {
			CLUSTER[at0010] occurrences matches {0..1} matches {    -- Details
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Speed
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::338]>
								list = <
									["1"] = <
										units = <"m/s">
									>
									["2"] = <
										units = <"km/h">
									>
									["3"] = <
										units = <"[mi_i]/h">
									>
									["4"] = <
										units = <"[ft_i]/s">
									>
								>
							>
						}
					}
					CLUSTER[at0015] occurrences matches {0..1} matches {    -- Pace
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0016] occurrences matches {0..1} matches {    -- Time per distance
								value matches {
									DV_DURATION matches {*}
								}
							}
							ELEMENT[at0017] occurrences matches {0..1} matches {    -- Distance
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::122]>
										list = <
											["1"] = <
												units = <"km">
											>
											["2"] = <
												units = <"[mi_i]">
											>
											["3"] = <
												units = <"m">
											>
											["4"] = <
												units = <"[yd_i]">
											>
											["5"] = <
												units = <"[ft_i]">
											>
										>

										assumed_value = <
											units = <"km">
											magnitude = <1.0>
											precision = <-1>
										>
									>
								}
							}
						}
					}
					ELEMENT[at0018] occurrences matches {0..1} matches {    -- Cadence
						value matches {
							C_DV_QUANTITY <
								
								list = <
									["1"] = <
										units = <"2[pi]rad/min">
									>
									["2"] = <
										units = <"/min">
									>
								>
							>
						}
					}
				}
			}
			CLUSTER[at0002] occurrences matches {0..1} matches {    -- Cumulative Details
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0012] occurrences matches {0..1} matches {    -- Distance
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"m">
									>
									["2"] = <
										units = <"km">
									>
									["3"] = <
										units = <"[mi_i]">
									>
									["4"] = <
										units = <"[yd_i]">
									>
									["5"] = <
										units = <"[ft_i]">
									>
								>
							>
						}
					}
					ELEMENT[at0019] occurrences matches {0..1} matches {    -- Count
						value matches {
							DV_COUNT matches {*}
						}
					}
				}
			}
		}
	}

