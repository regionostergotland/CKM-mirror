definition
	OBSERVATION[at0000] matches {	-- TNM stage - clinical
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Cancer type
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- Anatomical site
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Anatomical subsite
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Primary tumour (cT)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Regional lymph nodes (cN)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Distant metastasis (cM)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- Histopathological grading (G)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Residual tumour (R)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {	-- Lymphatic invasion (L)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {	-- Venous invasion (V)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0015] occurrences matches {0..*} matches {	-- Other descriptor
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {	-- Stage
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Stage grouping
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {	-- Retreatment (r)
										value matches {
											DV_BOOLEAN matches {
												value matches {True, False}
											}
										}
									}
									ELEMENT[at0019] occurrences matches {0..1} matches {	-- Multimodality therapy (y)
										value matches {
											DV_BOOLEAN matches {
												value matches {True, False}
											}
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {	-- Autopsy (a)
										value matches {
											DV_BOOLEAN matches {
												value matches {True, False}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0021] occurrences matches {0..*} matches {	-- TNM Edition
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0022] occurrences matches {0..*} matches {	-- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"TNM stage - clinical">
					description = <"A framework for the clinical classification and grading of malignancies using the TNM classification system.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0005"] = <
					text = <"Cancer type">
					description = <"The type of cancer being staged.">
					comment = <"Coding of the type of the cancer with a terminology is strongly preferred.">
				>
				["at0006"] = <
					text = <"Anatomical site">
					description = <"The anatomical site where the assessed cancer is situated.">
				>
				["at0007"] = <
					text = <"Anatomical subsite">
					description = <"The anatomical subsite where the assessed cancer is situated.">
				>
				["at0008"] = <
					text = <"Primary tumour (cT)">
					description = <"Clinical assessment of the primary tumour.">
				>
				["at0009"] = <
					text = <"Regional lymph nodes (cN)">
					description = <"Clinical assessment of the regional lymph nodes.">
					comment = <"May include suffixes. For example: sentinel nodes (sn); isolated tumor cells (ITCs) (i+); and FNA or a core biopsy (f).">
				>
				["at0010"] = <
					text = <"Distant metastasis (cM)">
					description = <"Clinical assessment of distant metastasis.">
				>
				["at0011"] = <
					text = <"Histopathological grading (G)">
					description = <"Histopathological grading of the tumour.">
				>
				["at0012"] = <
					text = <"Residual tumour (R)">
					description = <"Clinical assessment of post-treatment residual tumour.">
				>
				["at0013"] = <
					text = <"Lymphatic invasion (L)">
					description = <"Clinical assessment of invasion into the lymphatic system.">
				>
				["at0014"] = <
					text = <"Venous invasion (V)">
					description = <"Clinical assessment of invasion into the venous system.">
				>
				["at0015"] = <
					text = <"Other descriptor">
					description = <"Any other descriptor used in the clinical assessment.">
				>
				["at0016"] = <
					text = <"Stage">
					description = <"Concatenation of cT, cN and cM categories plus any of G, R, L and V, if applicable.">
				>
				["at0017"] = <
					text = <"Stage grouping">
					description = <"Clinical stage grouping.">
				>
				["at0018"] = <
					text = <"Retreatment (r)">
					description = <"Clinical staging is performed for a recurring cancer after a disease-free interval.">
				>
				["at0019"] = <
					text = <"Multimodality therapy (y)">
					description = <"Clinical staging is performed during or following initial multimodality therapy.">
				>
				["at0020"] = <
					text = <"Autopsy (a)">
					description = <"Clinical staging is performed by postmortem examination after the death of the patient.">
				>
				["at0021"] = <
					text = <"TNM Edition">
					description = <"The edition of the TNM classification system used for the assessment.">
				>
				["at0022"] = <
					text = <"Extension">
					description = <"Additional information required to capture local content or to align with other reference models/formalisms.">
