definition
    EVALUATION[at0000.1] matches {
        data matches {
            ITEM_TREE[at0001] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[at0002.1] occurrences matches {0..1} matches {
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0003.1] occurrences matches {0..*} matches {
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0.2] occurrences matches {0..1} matches {
                        value matches {
                            DV_TEXT matches {*}
                        }
                    }
                    ELEMENT[at0.1] occurrences matches {0..1} matches {
                        value matches {
                            DV_DATE_TIME matches {*}
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[at0004] matches {
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[at0.3] occurrences matches {0..1} matches {
                        value matches {
                            DV_MULTIMEDIA matches {
                                media_type matches {
                                    [openEHR::
                                    518,
                                    423,
                                    417,
                                    416]
                                }
                            }
                        }
                    }
                    allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

